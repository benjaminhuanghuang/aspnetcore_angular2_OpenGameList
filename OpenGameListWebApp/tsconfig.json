{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "removeComments": false,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [ "es2015", "dom" ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "wwwroot",
    "typings/main",
    "typings/main.d.ts"
  ]
}
