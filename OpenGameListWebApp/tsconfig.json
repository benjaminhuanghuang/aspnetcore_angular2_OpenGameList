{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "system",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "removeComments": false,
    "sourceRoot": "Scripts/app",
    "outDir": "wwwroot/app"
  },
  "exclude": [
    "node_modules",
    "dist",
    "wwwroot"
  ]
}
